{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "rawdata",
						"type": "DatasetReference"
					},
					"name": "readrawdata"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "filter1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ORDERNUMBER as string,",
				"          QUANTITYORDERED as string,",
				"          PRICEEACH as string,",
				"          ORDERLINENUMBER as string,",
				"          SALES as integer,",
				"          ORDERDATE as string,",
				"          STATUS as string,",
				"          QTR_ID as string,",
				"          MONTH_ID as string,",
				"          YEAR_ID as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> readrawdata",
				"readrawdata filter(SALES>=1500) ~> filter1"
			]
		}
	}
}